<?xml version="1.0"?>

<PropertyList>

    <path>p51d-jw-man.ac</path>

    <animation>
        <type>material</type>
        <object-name>face</object-name>
        <object-name>needle</object-name>
        <emission>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.4</blue>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>bezel</object-name>
        <emission>
            <factor-prop>/controls/lighting/cabin-norm</factor-prop>
            <red>0.2</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>engines/engine/mp-osi</property>
        <min>15</min>
        <max>100</max>
        <interpolation>
            <entry>
                <ind>15</ind>
                <dep>-60.0</dep>
            </entry>
            <entry>
                <ind>100</ind>
                <dep>290</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1</x>
        </axis>
    </animation>

</PropertyList>
